<configuration>
    <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{mm:ss.SSS}|%highlight(%-5level)|%green(%t)|%blue(%class{10}.%method)|%msg%n</pattern>
        </encoder>
    </appender>
    <root level="debug">
        <appender-ref ref="stdout" />
    </root>
<!--      <logger name="model" level="debug">-->
<!--        <appender-ref ref="stdout" />-->
<!--      </logger>-->
</configuration>